{
  "name": "better-build",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "remix dev -c \"npm run dev:server\" --manual",
    "dev:server": "tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server.ts",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=esm ./server.ts --outdir=./",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/server.js"
  ]
}
